# Package visibility settings
access=public

# Dependency management
save-exact=true
package-lock=true

# Publishing settings
git-tag-version=true
tag-version-prefix="v"
message="chore(release): %s"

# Security settings
audit=true
fund=false

# Performance settings
prefer-offline=true
cache-min=10800

# Scoped package settings
@teerai:registry=https://registry.npmjs.org/